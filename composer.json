{
    "name": "majora/ota_server",
    "description": "LinkValue store for mobile applications",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "LinkValue Partners",
            "homepage": "https://github.com/orgs/LinkValue/people"
        },
        {
            "name":     "LinkValue",
            "email":    "contact@link-value.fr",
            "homepage": "http://link-value.fr"
        }
    ],
    "autoload": {
        "psr-4": { "Majora\\OTAStore\\": "src/" },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "2.8.*",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "stof/doctrine-extensions-bundle": "^1.2",
        "doctrine/migrations": "^1.4",
        "doctrine/doctrine-migrations-bundle": "^1.1",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "hautelook/alice-bundle": "^1.3",
        "friendsofsymfony/jsrouting-bundle": "^2.0@alpha",
        "willdurand/js-translation-bundle": "~2.5"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "phpunit/phpunit": "^4.8",
        "symfony/phpunit-bridge": "^2.8",
        "behat/behat": "~3.0",
        "behat/symfony2-extension": "~2.1",
        "behat/mink-extension": "^2.1",
        "behat/mink-browserkit-driver": "^1.3"
    },
    "scripts": {
        "set-parameters-yml": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "setup-bootstrap": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "5.5.9"
        }
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.8-dev"
        }
    }
}
